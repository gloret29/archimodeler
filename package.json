{
  "name": "archimodeler",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test-db": "ts-node scripts/test-db-connection.ts",
    "db:generate": "npm run generate --workspace=@repo/database",
    "db:push": "npm run push --workspace=@repo/database"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.4.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3"
  }
}
